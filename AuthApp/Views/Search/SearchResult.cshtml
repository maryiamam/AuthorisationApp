@model AuthApp.Models.ViewModels.SearchViewModel

@{
    ViewBag.Title = "Search results";
}
<div class="jumbotron">
    <h3>@ViewBag.Title</h3>
</div>

<div class="results-count"></div>
@if(!string.IsNullOrEmpty(Model.Phrase))
{
    <div class="search-results" id="searchResults" data-search-string="@Model.Phrase">
    </div>
}

@section scripts
{
    @Scripts.Render("~/bundles/scroll")
}